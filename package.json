{
  "name": "figma-google-sheets-chart-plugin",
  "version": "1.0.0",
  "description": "A Figma plugin to import and update Google Sheets charts",
  "main": "dist/code.js",
  "scripts": {
    "build": "tsc -p tsconfig.json && cp ui/ui.html dist/ui.html",
    "watch": "tsc -p tsconfig.json --watch",
    "dev": "npm run build && figma plugin start",
    "clean": "rm -rf dist && mkdir -p dist"
  },
  "devDependencies": {
    "@figma/plugin-typings": "^1.82.0",
    "typescript": "^5.0.0"
  },
  "keywords": [
    "figma",
    "plugin",
    "google-sheets",
    "charts"
  ],
  "author": "Your Name",
  "license": "MIT"
}
